{
  "name": "node-webvtt",
  "version": "1.9.3",
  "description": "WebVTT parser, compiler, and segmenter with HLS support",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint *.js **/*.js",
    "test": "npm run eslint -s && nyc mocha -R dot"
  },
  "keywords": [
    "webvtt",
    "vtt",
    "segment",
    "hls",
    "subtitle",
    "closed",
    "caption"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/osk/node-webvtt"
  },
  "author": "Ã“lafur Sverrir Kjartansson <kjarni@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.3.0",
    "eslint": "^7.20.0",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "commander": "^7.2.0"
  },
  "engines": {
    "node": ">= 8.16.0"
  },
  "files": [
    "/lib",
    "/bin",
    "index.js"
  ],
  "bin": {
    "webvtt-segment": "./bin/webvtt-segment.js"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "lines": 100
  }
}
